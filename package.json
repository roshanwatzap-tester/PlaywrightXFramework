{
  "name": "playwrightautomation",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "webtests": "npx playwright test --grep @Web --reporter=line,allure-playwright",
    "allure:generate": "allure generate ./allure-results --clean",
    "allure:open": "allure open ./allure-report",
    "allure:report": "npm run allure:generate && npm run allure:open",
    "AItests": "npx playwright test --grep @AI",
    "regression": "npx playwright test",
    "smoke": "npx playwright test tests/E2E_PO.spec.js",
    "runai" : "npx playwright test --config playwright.AIconfig.js",
    "SafariNewConfig": "npx playwright test --config playwright.config1.js --project=safari"
  },
