  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@types/node": "^22.7.8",
    "@zerostep/playwright": "^0.1.5",
    "allure-playwright": "^3.3.3",
    "multiple-cucumber-html-reporter": "^3.9.3"
  },
  "dependencies": {
    "@cucumber/cucumber": "^12.1.0",
    "dotenv": "^17.2.1",
    "exceljs": "^4.4.0",
    "http-server": "^14.1.1",
    "mysql2": "^3.14.3"
  },
